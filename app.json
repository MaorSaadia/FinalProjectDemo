{
  "expo": {
    "name": "FinalProjectDemo",
    "slug": "FinalProjectDemo",
    "version": "0.3.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ebe6df"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#e3ebee"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.maorsa.FinalProjectDemo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "אפשר לאפליקציה לגשת לתמונות שלך",
          "cameraPermission": "אפשר לאפליקציה לגשת למצלמה שלך"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3c40626b-bb3b-42f7-bc7d-4986f1177297"
      }
    }
  }
}
